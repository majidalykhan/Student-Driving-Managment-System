<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".StudentReport">


    <TextView
        android:id="@+id/selectDateText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Comp hours:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.861" />

    <TextView
        android:id="@+id/selectDateText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total Fee:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.081"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.678" />

    <TextView
        android:id="@+id/selectDateText7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total Paid:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.705"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.766" />

    <TextView
        android:id="@+id/selectDateText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Balance:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.678" />

    <TextView
        android:id="@+id/selectDateText8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hours left:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.701"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.86" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText3"
        app:layout_constraintStart_toEndOf="@+id/selectDateText3"
        app:layout_constraintTop_toTopOf="@+id/selectDateText3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText4"
        app:layout_constraintStart_toEndOf="@+id/selectDateText4"
        app:layout_constraintTop_toTopOf="@+id/selectDateText4"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText5"
        app:layout_constraintStart_toEndOf="@+id/selectDateText5"
        app:layout_constraintTop_toTopOf="@+id/selectDateText5"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText6"
        app:layout_constraintStart_toEndOf="@+id/selectDateText6"
        app:layout_constraintTop_toTopOf="@+id/selectDateText6"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText7"
        app:layout_constraintStart_toEndOf="@+id/selectDateText7"
        app:layout_constraintTop_toTopOf="@+id/selectDateText7"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/selectDateText8"
        app:layout_constraintStart_toEndOf="@+id/selectDateText8"
        app:layout_constraintTop_toTopOf="@+id/selectDateText8"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/selectDateText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Discount:"
        android:textColor="#000"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.08"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.768" />

    <ImageButton
        android:id="@+id/exportReportBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.956"
        app:srcCompat="@drawable/ic_button"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/exportReportBtnText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Export Report"
        android:textColor="#ffffff"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/exportReportBtn"
        app:layout_constraintEnd_toEndOf="@+id/exportReportBtn"
        app:layout_constraintHorizontal_bias="0.510"
        app:layout_constraintStart_toStartOf="@+id/exportReportBtn"
        app:layout_constraintTop_toTopOf="@+id/exportReportBtn"
        app:layout_constraintVertical_bias="0.4" />

    <TableLayout
        android:layout_width="410dp"
        android:layout_height="362dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/holo_green_light">

            <TextView
                android:id="@+id/dateColom"
                android:layout_weight="1"
                android:background="#dff6f0"
                android:gravity="center"
                android:padding="20dip"
                android:text="Date"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/hoursColom"
                android:layout_weight="1"
                android:background="#dff6f0"
                android:gravity="center"
                android:padding="20dip"
                android:text="Hours"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/feeColom"
                android:layout_weight="1"
                android:background="#dff6f0"
                android:gravity="center"
                android:padding="20dip"
                android:text="Total Fee"
                android:textColor="#000000" />

            <TextView
                android:id="@+id/paidamountColom"
                android:layout_weight="1"
                android:background="#dff6f0"
                android:gravity="center"
                android:padding="20dip"
                android:text="Paid Amount"
                android:textColor="#000000" />

        </TableRow>


    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>